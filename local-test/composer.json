{
    "name": "sergkeim/local-test",
    "description": "A test project for sergkeim/php-cloudflare-api",
    "authors": [
        {
            "name": "Sergkei Melingk",
            "email": "sergio11of@gmail.com",
            "homepage": "https://github.com/SergkeiM"
        }
    ],
    "autoload": {
        "psr-4": {
            "SergkeiM\\LocalTest\\": "src/"
        }
    },
    "require": {
        "sergkeim/php-cloudflare-api": "*",
        "guzzlehttp/guzzle": "^7.9",
        "vlucas/phpdotenv": "^5.6"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
