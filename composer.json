{
    "name": "sergkeim/php-cloudflare-api",
    "description": "A simple PHP CloudFlare API client.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sergkei Melingk",
            "email": "sergio11of@gmail.com",
            "homepage": "https://github.com/SergkeiM"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^7.9",
        "guzzlehttp/uri-template": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.4",
        "php-http/mock-client": "^1.6",
        "symfony/phpunit-bridge": "^7.1",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "SergkeiM\\CloudFlare\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "SergkeiM\\CloudFlare\\Tests\\": "test/Tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "pint": "vendor/bin/pint"
    }
}
